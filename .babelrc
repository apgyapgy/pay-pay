{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "es2015",
    "stage-2"
  ],
  "plugins": ["babel-plugin-transform-remove-strict-mode","transform-runtime"],
  "env": {
    "test": {
      "presets": ["env","es2015", "stage-2"],
      "plugins": ["istanbul"]
    }
  }
}
